#include<stdio.h>
int main(){
    int n, temp, rev =0, rem;
    scanf("%d", &n);
    temp = n;
     while (temp >0){
        rem = temp % 10;
        rev = rev *10 + rem;
        temp /= 10;
     }
       if (rev == n)
       printf("palindrome");
       else
       printf("not palindrome");
       return 0;
    } 